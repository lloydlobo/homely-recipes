{
  "name": "homely-recipes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "del -f ./dist && webpack",
    "clean": "npx del dist",
    "dependencies:global": "npm list -g --depth 0",
    "dependencies:local": "npm list --depth 0",
    "deploy": "node ./scripts/deploy",
    "dev": "webpack serve",
    "jsdoc": "./node_modules/.bin/jsdoc src/**.*js",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "locale-snapshots": "env TZ=utc babel-node --extensions .ts,.js ./scripts/build/localeSnapshots/index.js",
    "prebuild": "npm run clean",
    "prepare": "husky install",
    "stats": "cloc . --exclude-dir=node_modules,tmp,.git",
    "test": "npm run lint && npm run test:jest",
    "test:jest": "jest",
    "test:karma": "karma start karma.conf.js",
    "update-schema": "node ./scripts/update-schema"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lloydlobo/homely-recipes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lloydlobo/homely-recipes/issues"
  },
  "homepage": "https://github.com/lloydlobo/homely-recipes#readme",
